    "debug": "^4.3.7",
    "needle": "^3.3.1",
    "sharp": "^0.32.5",
    "starlight-package-managers": "^0.7.0",
    "typescript": "^5.6.3",
    "unist-util-visit": "^5.0.0"
  }
